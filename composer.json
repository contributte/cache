{
  "name": "contributte/cache",
  "description": "Extra contrib to nette/cache",
  "keywords": [
    "nette",
    "cache",
    "provider"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/cache",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">= 7.1",
    "nette/caching": "~2.5.8 || ~3.0.0"
  },
  "require-dev": {
    "nette/di": "~2.4.13",
    "ninjify/nunjuck": "^0.2.0",
    "ninjify/qa": "^0.8.0",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-nette": "^0.11",
    "phpstan/phpstan-shim": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11"
  },
  "suggest": {
    "nette/di": "to use CacheFactoryExtension[CompilerExtension]"
  },
  "conflict": {
    "nette/di": "<2.4.13",
    "nette/utils": "<2.5.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Cache\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4.x-dev"
    }
  }
}
