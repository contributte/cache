{
  "name": "contributte/cache",
  "description": "Extra contrib to nette/cache",
  "keywords": [
    "nette",
    "cache",
    "provider"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/cache",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "nette/caching": "^3.0.0"
  },
  "require-dev": {
    "nette/di": "~3.0.0",
    "ninjify/nunjuck": "^0.4",
    "ninjify/qa": "^0.12",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-deprecation-rules": "^0.12",
    "phpstan/phpstan-nette": "^0.12",
    "phpstan/phpstan-strict-rules": "^0.12",
    "tracy/tracy": "^2.6.0"
  },
  "suggest": {
    "nette/di": "to use CacheFactoryExtension and DebugStorageExtension"
  },
  "conflict": {
    "nette/di": "<3.0.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Cache\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.7.x-dev"
    }
  }
}
